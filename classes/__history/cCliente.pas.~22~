unit cCliente;

interface

uses
  System.Classes, Vcl.Controls, Vcl.ExtCtrls, Vcl.Dialogs, System.SysUtils, datamodule, Data.DB;

type
  TcCliente = Class
    private
      FId: integer;
      FCpf: string;
      FNome: string;


    public
     class function CheckExistenceByCPF(const cpf: string): Boolean;

    property id: integer read FId write FId;
    property cpf: string read FCpf write FCpf;
    property nome: string read FNome write FNome;

  End;

implementation

class function TcCliente.CheckExistenceByCPF(const cpf: string): Boolean;
begin
  with uDataModule.qryClienteGetByCpf do
  begin
    Close;
      Parameters.ParamByName('cpf').Value := cpf;
    Open;
    ShowMessage(FieldByName('nome').AsString);
    Result := not EOF;
  end;
end;



end.
